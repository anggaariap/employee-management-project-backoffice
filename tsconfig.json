{
  "compilerOptions": {
    "target": "ES2022",
    "useDefineForClassFields": false,
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": false,
    "types": [],
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": false
  }
}